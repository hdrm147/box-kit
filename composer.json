{
    "name": "hdrm147/box-kit",
    "description": "A Laravel Nova field and tool for box picking and purchase advising.",
    "keywords": ["laravel", "nova", "box", "packing", "shipping"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
      "laravel/nova-devtool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hdrm147\\BoxKit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hdrm147\\BoxKit\\BoxKitServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
